{
	"manifest_version": 2,
	"name": "EoT Facebook Names to Toon Names",
	"version": "1.0",
        "icons": {
        "128": "128.png"
        },
        "permissions": ["tabs","*://*.facebook.com/groups/temperanceguild/","webNavigation"],
